# Database
DATABASE_URL="postgresql://user:password@localhost:5432/youtube_analytics"

# Google OAuth (for authenticated access)
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
GOOGLE_REDIRECT_URI="http://localhost:3000/auth/callback"

# YouTube Data API (for public access)
# Get your API key from https://console.cloud.google.com/
# Used by /public-analytics page
YOUTUBE_API_KEY="your-youtube-api-key"
NEXT_PUBLIC_YOUTUBE_API_KEY="your-youtube-api-key"

# Redis (optional, for caching)
KV_REST_API_URL="redis://localhost:6379"
KV_REST_API_TOKEN=""

# Encryption
CHANNEL_ENCRYPTION_KEY="your-32-character-encryption-key"

# Cron Jobs
CRON_SECRET_TOKEN="your-cron-secret-token"

# Development
NODE_ENV="development"