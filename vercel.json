{
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps --production=false",
  "framework": "nextjs",
  "env": {
    "SKIP_ENV_VALIDATION": "true"
  },
  "functions": {
    "app/api/health/route.ts": {
      "maxDuration": 10
    },
    "app/api/channels/sync-all/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/*/route.ts": {
      "maxDuration": 60
    }
  }
}